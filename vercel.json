{
  "version": 2,
  "alias": ["promptadvisers-voice.vercel.app"],
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 60
      }
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/s/(.*)",
      "dest": "/public/s.html"
    },
    {
      "src": "/s$",
      "dest": "/public/s.html"
    },
    {
      "src": "/player.html",
      "dest": "/public/player.html"
    },
    {
      "src": "/player-script.js",
      "dest": "/public/player-script.js"
    },
    {
      "src": "/(.*\\.js)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*\\.css)",
      "dest": "/public/$1"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    }
  ]
}
